#map{
  width:100%;
  height: 100%;
}
/*start -顶部提示部分*/  
.tip-bar {
  width: 700rpx;
  height: 90rpx;
  position: fixed;
  left: 25rpx;
  top: 38rpx;
  background-color:#454545;
  border-radius: 45rpx;
}
.tip-list{display: inline-flex;}
.tip-image,.tip-text{
  display:inline-flex;
}
.tip-image{
  margin-left: 27rpx;
  margin-top: 15rpx;
  width:48rpx;
  height:50rpx ;
}
.tip-text{  
  margin-left: 20rpx;
  margin-top: 33rpx;
  margin-bottom: 33rpx;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 27rpx;
  color: #fff;
  font-family: PingFangSC-Regular, sans-serif;  
}
.tip-store{
  color:#ff7200;
  font-weight: bold;
}
